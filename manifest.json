{
    "manifest_version": 2,
    "name": "ChatGPT to PDF",
    "version": "1.0",
    "description": "Convierte un chat de ChatGPT a PDF",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "browser_action": {
      "default_icon": {
        "16": "images/1.png",
        "48": "images/2.png",
        "128": "images/3.png"
      },
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "images/1.png",
      "48": "images/2.png",
      "128": "images/3.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  